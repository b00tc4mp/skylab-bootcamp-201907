{
  "name": "menu-planner-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "menu-planner-data": "file:../menu-planner-data",
    "menu-planner-utils": "file:../menu-planner-utils",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:inspect": "react-scripts --inspect-brk=9230 test --runInBand --no-cache",
    "test:coverage": "react-scripts test --coverage --watchAll"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
    "momentjs": "^2.0.0"
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "src/common"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/components/**/*.js",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ]
  }
}
